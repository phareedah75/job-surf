{
  "name": "bw-fullstck-reference-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "local": "ng serve --proxy-config proxy.conf.local.json --port 4200",
    "clean:api-sdk": "rimraf *.tgz ./sdk/bw-fullstack-reference-api-sdk",
